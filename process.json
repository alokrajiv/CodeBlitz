{
    "name": "worker",
    "script": "bin/server.js",
    "instances": 1,
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm Z",
    "watch": [
        "bin/server.js",
        "*"
    ],
    "watch_options": {
        "followSymlinks": true,
        "usePolling": true,
        "interval": 5
    }
}